    "repositories": {
        "0": {
            "url": "https://packagist.org",
            "type": "composer"
        },
        "1": {
            "type": "composer",
            "url": "https://packages.drupal.org/8"
        },
        "2": {
            "type": "path",
            "url": "web/modules/composer_modified/default_content"
        },
        "asset-packagist": {
            "type": "composer",
            "url": "https://asset-packagist.org"
        }
    },
    "scripts": {
        "post-install-cmd": [
            "Acquia\\Lightning\\ScriptHandler::deployLibraries"
        ],
        "post-update-cmd": [
            "Acquia\\Lightning\\ScriptHandler::deployLibraries"
        ]
    },
    "autoload": {
        "classmap": [
            "scripts/composer/ScriptHandler.php",
            "scripts/acquia/ScriptHandler.php"
        ]
    },
    "extra": {
        "installer-paths": {
            "web/core": [
                "type:drupal-core"
            ],
            "web/modules/contrib/{$name}": [
                "type:drupal-module"
            ],
            "web/profiles/contrib/{$name}": [
                "type:drupal-profile"
            ],
            "web/themes/contrib/{$name}": [
                "type:drupal-theme"
            ],
            "drush/contrib/{$name}": [
                "type:drupal-drush"
            ],
            "docroot/libraries/{$name}": [
                "type:bower-asset",
                "type:npm-asset"
            ]
        },
        "enable-patching": true,
        "patches": [],
        "installer-types": [
            "bower-asset",
            "npm-asset"
        ]
    },
    "replace": {
        "drupal/drupal": "*"
    }
