{
    "name": "digital-immersion/infogasm",
    "description": "Infogasm, a fun blog.",
    "type": "project",
    "license": "GPL-2.0+",
    "minimum-stability": "dev",
    "prefer-stable": true,
    "require": {
        "php": "~5.6.5|7.0.2|7.0.4|~7.0.6|~7.1|~7.2",
        "drupal-composer/drupal-scaffold": "^2.5",
        "cweagans/composer-patches": "^1.6",
        "drupal/google_analytics": "^2.2",
        "drupal/redirect": "^1.0",
        "drupal/recaptcha": "2.x-dev",
        "drupal/console": "^1.8",
        "drupal/pathauto": "^1.3",
        "drush/drush": "^9",
        "drupal/coffee": "^1.0@beta",
        "drupal/webform": "^5.0",
        "drupal/core": "8.6.4",
        "drupal/metatag": "^1.8"
    },
    "autoload": {
        "classmap": [
            "scripts/composer/ScriptHandler.php"
        ]
    },
    "repositories": {
        "drupal": {
            "type": "composer",
            "url": "https://packages.drupal.org/8"
        }
    },
    "extra": {
        "installer-paths": {
            "web/core": [
                "type:drupal-core"
            ],
            "web/modules/contrib/{$name}": [
                "type:drupal-module"
            ],
            "web/profiles/contrib/{$name}": [
                "type:drupal-profile"
            ],
            "web/themes/contrib/{$name}": [
                "type:drupal-theme"
            ],
            "drush/contrib/{$name}": [
                "type:drupal-drush"
            ]
        },
        "enable-patching": true,
        "patches": [],
        "installer-types": [
            "bower-asset",
            "npm-asset"
        ]
    },
    "replace": {
        "drupal/drupal": "*"
    }
}
